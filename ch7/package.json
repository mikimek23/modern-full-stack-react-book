    "dotenv": "^17.2.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "express": "^5.2.1",
    "jwt-decode": "^4.0.0",
    "lint-staged": "^16.2.7",
    "lucide-react": "^0.563.0",
    "prop-types": "^15.8.1",
